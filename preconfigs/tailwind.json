{
    "tailwindCSS.includeLanguages": {
        "plaintext": "html",
        "javascript": "javascript",
        "javascriptreact": "javascript",
        "typescript": "typescript",
        "typescriptreact": "typescript"
    },
    "files.associations": {
        "*.html": "html",
        "*.js": "javascript",
        "*.jsx": "javascriptreact",
        "*.ts": "typescript",
        "*.tsx": "typescriptreact",
        "*.css": "tailwindcss"
    },
    "tailwindCSS.emmetCompletions": true
}